                    <div class="server_time left">
                        Server time: <span id="clock"></span>
                        <script type="text/javascript">
                            function upd_tz() {
                                var d = new Date();
                                document.cookie = 'tz=' + escape(d.getTimezoneOffset()) + "; path=/";
                                return true;
                            }
                            obj_clock = document.getElementById("clock");
                            time_h = 19;
                            time_m = 46;
                            time_s = 52;

                            function dig2(d) {
                                return ((d < 10) ? "0" : "") + d;
                            }

                            function wr_clock() {
                                obj_clock.innerHTML = dig2(time_h) + ':' + dig2(time_m) + ':' + dig2(time_s);
                                time_s++;
                                if (time_s >= 60) {
                                    time_s = 0;
                                    time_m++;
                                    if (time_m >= 60) {
                                        time_m = 0;
                                        time_h++;
                                        if (time_h >= 24) {
                                            time_h = 0;
                                        }
                                    }
                                }
                            }
                            wr_clock();
                            setInterval("wr_clock();", 1000);
                        </script>
